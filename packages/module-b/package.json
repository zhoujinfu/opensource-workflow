{
  "name": "module-b",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts",
    "*.mjs",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "tdd": "vitest -w",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "build": "tsup src/{react,vue,index,node}.ts --format cjs,esm --dts --clean --target node14",
    "watch": "pnpm build -- --watch src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^5.12.6"
  }
}
