{
  "name": "@tinychou/module-c",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts",
    "*.mjs",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
