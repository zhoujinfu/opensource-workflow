    "prepublishOnly": "pnpm build",
    "build": "node -e \"const fs = require('fs-extra');fs.remove('./dist');\" && rollup --configPlugin typescript --config rollup.config.ts",
    "watch": "pnpm build -- --w"
  },
  "keywords": [],
